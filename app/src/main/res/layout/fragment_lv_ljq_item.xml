<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_device_details"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.activity.DeviceDetailsActivity">
    <RelativeLayout
        android:id="@+id/rl_station_info"
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:background="#ffffffff" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="0dp"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/textView10"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:paddingLeft="15dp"
                        android:text="二维码信息："
                        android:textColor="@color/commonly_text_color3"
                        android:textSize="15sp" />

                    <TextView
                        android:id="@+id/erweima"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:textColor="@color/commonly_text_color3"
                        android:textSize="15sp"
                        android:gravity="left|center_vertical"
                        android:text="" />
                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="horizontal" >


                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1.2"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:textColor="@color/commonly_text_color3"
                        android:layout_marginLeft="1dp"
                        android:layout_marginTop="1dp"
                        android:background="#ffffffff"
                        android:textSize="15sp"
                        android:gravity="left|center_vertical"
                        android:paddingLeft="15dp"
                        android:text="插头组件型号: " />

                    <TextView
                        android:id="@+id/xinghao"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:textColor="@color/commonly_text_color3"
                        android:layout_marginRight="1dp"
                        android:textSize="15sp"
                        android:layout_marginTop="1dp"
                        android:background="#ffffffff"
                        android:gravity="left|center_vertical"
                        android:text="" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/textView1"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:textColor="@color/commonly_text_color3"
                        android:layout_marginTop="1dp"
                        android:background="#ffffffff"
                        android:textSize="15sp"
                        android:gravity="left|center_vertical"
                        android:paddingLeft="15dp"
                        android:text="插头组件编号: " />

                    <TextView
                        android:id="@+id/bianhao"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:textColor="@color/commonly_text_color3"
                        android:layout_marginRight="1dp"
                        android:textSize="15sp"
                        android:layout_marginTop="1dp"
                        android:layout_weight="1"
                        android:background="#ffffffff"
                        android:gravity="left|center_vertical"
                        android:text="" />
                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="horizontal" >
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1.2"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="1dp"
                        android:layout_marginTop="1dp"
                        android:background="#ffffffff"
                        android:gravity="left|center_vertical"
                        android:paddingLeft="15dp"
                        android:text="插头组件制造厂家："
                        android:textColor="@color/commonly_text_color3"
                        android:textSize="15sp" />

                    <TextView
                        android:id="@+id/changjia"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:textColor="@color/commonly_text_color3"
                        android:textSize="15sp"
                        android:layout_marginRight="1dp"
                        android:layout_marginTop="1dp"
                        android:background="#ffffffff"
                        android:gravity="left|center_vertical"
                        android:text="" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/textView4"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:textColor="@color/commonly_text_color3"
                        android:textSize="15sp"
                        android:layout_marginTop="1dp"
                        android:background="#ffffffff"
                        android:gravity="left|center_vertical"
                        android:paddingLeft="15dp"
                        android:text="插头组件铅封日期：" />

                    <TextView
                        android:id="@+id/qianfeng"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:textColor="@color/commonly_text_color3"
                        android:textSize="15sp"
                        android:layout_marginRight="1dp"
                        android:layout_marginTop="1dp"
                        android:layout_weight="1"
                        android:background="#ffffffff"
                        android:gravity="left|center_vertical"
                        android:text="" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1.2"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:textColor="@color/commonly_text_color3"
                        android:textSize="15sp"
                        android:gravity="left|center_vertical"
                        android:paddingLeft="15dp"
                        android:text="接口类型：" />

                    <TextView
                        android:id="@+id/leixing"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:textColor="@color/commonly_text_color3"
                        android:textSize="15sp"
                        android:gravity="left|center_vertical"
                        android:text="" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/textView8"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:textColor="@color/commonly_text_color3"
                        android:textSize="15sp"
                        android:layout_marginBottom="1dp"
                        android:layout_marginTop="1dp"
                        android:gravity="left|center_vertical"
                        android:paddingLeft="15dp"
                        android:text="接口用途：" />

                    <TextView
                        android:id="@+id/yongtu"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:textColor="@color/commonly_text_color3"
                        android:textSize="15sp"
                        android:gravity="left|center_vertical"
                        android:text="" />
                </LinearLayout>





            </LinearLayout>




        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>




